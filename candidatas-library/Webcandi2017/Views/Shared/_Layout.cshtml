<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Candidatas a Reina 2017</title>
    <link href="~/Content/materialize.min.css" rel="stylesheet" type="text/css" />
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body class="grey lighten-4">
    <div class="navbar-brand">
        <nav class="teal">
            <div class="nav-wrapper">
                <div class="container">
                @Html.ActionLink("Candidatas a Reina 2017", "Index", "Candidata", new { area = "" }, new { @class = "brand-logo" })
                </div>
            </div>
        </nav>
    </div>

    <div class="container">
        @RenderBody()
    </div>
    <footer class="page-footer teal">
        <div class="footer-copyright">
            <div class="container">
                &copy; @DateTime.Now.Year - Desarollo de Aplicaciones III.
            </div>
        </div>
    </footer>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/materialize.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('select').material_select();
        });
    </script>
</body>
</html>